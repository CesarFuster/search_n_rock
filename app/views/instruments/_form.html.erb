<div class="container">
  <div>
    <%= simple_form_for @instrument do |f| %>
      <%= f.input :category %>
      <%= f.input :brand %>
      <%= f.input :description %>
      <%= f.input :condition %>
      <%= f.input :day_value %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <p></p>
      <%= f.submit %>
    <% end %>
   </div>
</div>
